{
  "name": "be-importing",
  "version": "0.0.44",
  "description": "With the advent of declarative shadow DOM, many useful web components that require little to no js could be less taxing on the browser if they were imported as pre-rendered HTML rather than JavaScript.",
  "main": "be-importing.js",
  "module": "be-importing.js",
  "keywords": [
    "web-components",
    "web-component",
    "custom-element",
    "custom-elements"
  ],
  "scripts": {
    "test": "playwright test",
    "package-check": "package-check",
    "serve": "node node_modules/may-it-serve/serve.js"
  },
  "exports": {
    ".": "./be-importing.js",
    "./be-importing.js": "./be-importing.js"
  },
  "files": [
    "*.*"
  ],
  "types": "types.d.ts",
  "dependencies": {
    "be-decorated": "0.0.211",
    "be-having": "0.0.4",
    "be-hive": "0.0.79",
    "be-written": "0.0.14"
  },
  "devDependencies": {
    "may-it-serve": "0.0.4",
    "@skypack/package-check": "0.2.2",
    "@playwright/test": "1.29.1",
    "@custom-elements-manifest/analyzer": "0.6.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bahrus/be-importing.git"
  },
  "author": "anderson.bruce.b@gmail.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bahrus/be-importing/issues"
  },
  "homepage": "https://github.com/bahrus/be-importing#readme"
}
